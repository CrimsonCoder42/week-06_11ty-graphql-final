---
title: Course 112559
layout: base
---

<!-- Course information -->
<h1>{{ course.name }}</h1>
<p>Course ID: {{ course.id }}</p>

<!-- Assignments -->
{% if course.assignmentsConnection.nodes.size > 0 %}
  <h2>Assignments</h2>
  <ul>
    {% for assignment in course.assignmentsConnection.nodes %}
      <li>
        <h3>{{ assignment.name }}</h3>
        <p>{{ assignment.description }}</p>
        <p>Due at: {{ assignment.dueAt | date: "%Y-%m-%d %H:%M:%S" }}</p>
      </li>
    {% endfor %}
  </ul>
{% endif %}

<!-- Modules -->
{% if course.modulesConnection.nodes.size > 0 %}
  <h2>Modules</h2>
  <ul>
    {% for module in course.modulesConnection.nodes %}
      <li>
        <h3>{{ module.name }}</h3>
        <ul>
          {% for item in module.items %}
            <li>
              <p>{{ item.title }} ({{ item.type }})</p>
            </li>
          {% endfor %}
        </ul>
      </li>
    {% endfor %}
  </ul>
{% endif %}
